{
    "name": "contao/contao",
    "type": "project",
    "description": "Contao Open Source CMS",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "contao/php-diff": "dev-master",
        "contao/contao-bundle": "dev-develop",
        "contao/contao-library": "dev-develop",
        "contao/module-calendar": "dev-develop",
        "contao/module-comments": "dev-develop",
        "contao/module-core": "dev-develop",
        "contao/module-faq": "dev-develop",
        "contao/module-listing": "dev-develop",
        "contao/module-news": "dev-develop",
        "contao/module-newsletter": "dev-develop",
        "contao/module-repository": "dev-develop",
        "contao-components/ace": "~1.1",
        "contao-components/chosen": "~1.0",
        "contao-components/colorbox": "~1.5",
        "contao-components/dropzone": "~3.8",
        "contao-components/highlighter": "~3.0",
        "contao-components/html5shiv": "~3.7",
        "contao-components/jquery": "~1.11",
        "contao-components/jquery-ui": "~1.11",
        "contao-components/mediaelement": "~2.14",
        "contao-components/mootools": "~1.5",
        "contao-components/swipe": "~2.0",
        "contao-components/tablesorter": "~2.0",
        "contao-components/tinymce4": "~4.0",
        "leafo/lessphp": "~0.4",
        "leafo/scssphp": "dev-master",
        "leafo/scssphp-compass": "dev-master",
        "mabrahamde/idna-converter": "dev-master",
        "michelf/php-markdown": "~1.4",
        "sensio/distribution-bundle": "~3.0",
        "simplepie/simplepie": "~1.3",
        "swiftmailer/swiftmailer": "~5.2",
        "symfony/console": "~2.5",
        "symfony/class-loader": "~2.5",
        "symfony/finder": "~2.5",
        "symfony/framework-bundle": "~2.5",
        "symfony/yaml": "~2.5",
        "tecnick.com/tcpdf": "~6.0"
    },
    "require-dev": {
        "contao/contao-devtools": "dev-develop"
    },
    "replace": {
        "contao/core": "self.version"
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "components"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    }
}
